            <Grid x:Name="PopupAnnotationTab" Height="70" Visibility="Collapsed">
                <Grid Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Stretch">
                    <ToggleButton x:Name="PopupButton" Background="Transparent" Tag="Popup" Style="{StaticResource PdfViewerPopupAnnotationBtnStyle}" Checked="PopupButton_Checked" Unchecked="PopupButton_Unchecked">
                    </ToggleButton>
                </Grid>
            </Grid>
        </Grid>
        <Grid Grid.Row="1"  VerticalAlignment="Top"  Background="#DD262726">
            <Grid x:Name="TextMarkupAnnotationTab" Height="70" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Stretch">
                    <ToggleButton x:Name="HighlightButton" Background="Transparent" Tag="Highlight" Style="{StaticResource PdfViewerHighlightButtonStyle}" Checked="Highlight_Checked" Unchecked="Highlight_Unchecked">
                    </ToggleButton>
                </Grid>
                <Grid Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Stretch">
                    <ToggleButton x:Name="UnderlineButton" Background="Transparent" Tag="Underline" Style="{StaticResource PdfViewerUnderlineButtonStyle}" Checked="UnderlineButton_Checked" Unchecked="UnderlineButton_Unchecked">
                    </ToggleButton>
                </Grid>
                <Grid Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Stretch">
                    <ToggleButton x:Name="StrikethroughButton" Background="Transparent" Tag="Strikethrough" Style="{StaticResource PdfViewerStrikethroughButtonStyle}" Checked="StrikethroughButton_Checked" Unchecked="StrikethroughButton_Unchecked">
                    </ToggleButton>
                </Grid>
            </Grid>
        </Grid>

        <Grid Grid.Row="1"  VerticalAlignment="Top"  Background="#DD262726">
            <Grid x:Name="ShapeAnnotationTab" Height="70" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="0" HorizontalAlignment="Center" VerticalAlignment="Stretch">
                    <ToggleButton x:Name="LineButton" Background="Transparent" Tag="Line" Style="{StaticResource PdfViewerLineBtnStyle}" Checked="LineButton_Checked" Unchecked="LineButton_Unchecked">
                    </ToggleButton>
                </Grid>
                <Grid Grid.Column="1" HorizontalAlignment="Center" VerticalAlignment="Stretch">
                    <ToggleButton x:Name="RectangleButton" Background="Transparent" Tag="Rectangle" Style="{StaticResource PdfViewerRectangleBtnStyle}" Checked="RectangleButton_Checked" Unchecked="RectangleButton_Unchecked">
                    </ToggleButton>
                </Grid>
                <Grid Grid.Column="2" HorizontalAlignment="Center" VerticalAlignment="Stretch">
                    <ToggleButton x:Name="EllipseButton" Background="Transparent" Tag="Ellipse" Style="{StaticResource PdfViewerEllipseBtnStyle}" Checked="EllipseButton_Checked" Unchecked="EllipseButton_Unchecked">
                    </ToggleButton>
                </Grid>
            </Grid>
        </Grid>

        <Grid Grid.Row="4"  VerticalAlignment="Top"  Background="#DD262726">
            <Grid x:Name="TopSearchTab" Height="70" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="8.0*"></ColumnDefinition>
                    <ColumnDefinition Width="2.0*"></ColumnDefinition>
                </Grid.ColumnDefinitions>

                <Grid Grid.Column="0" >
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="10*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <Grid Grid.Column="0"  HorizontalAlignment="Stretch" >
                        <Grid  VerticalAlignment="Center">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="8*"></ColumnDefinition>
                                <ColumnDefinition Width="2*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>

                            <Grid Margin="10,0,0,0"  Grid.Column="0" HorizontalAlignment="Stretch" >
                                <TextBox  BorderThickness="0" Name="PageSearchTxtBox" Height="40" FontFamily="Segoe UI" FontSize="23" Background="#E4E4E4" VerticalAlignment="Center"  BorderBrush="#56A7D8" GotFocus="PageSearchTxtBox_GotFocus" KeyDown="PageSearchTxtBox_KeyDown" Style="{StaticResource SearchTextBoxStyle}"  />
                            </Grid>

                            <Grid Grid.Column="1"  HorizontalAlignment="Stretch">
                                <Button  x:Name="SearchIcon" Style="{StaticResource PdfViewerSearchIconStyle}" BorderThickness="0"  Tag="SearchIcon" Click="SearchIcon_Click"/>
                            </Grid>
                        </Grid>
                    </Grid>
                  

                </Grid>
                <Grid Grid.Column="1" x:Name="Close" >
                    <Button HorizontalAlignment="Center" Name="CloseButton" Height="40"  BorderBrush="#929292" FontSize="12" Foreground="#CDCDCD"  Content="Cancel" Tag="Close" BorderThickness="1"  Click="CloseSearch_Click"/>
                </Grid>
              
            </Grid>
        </Grid>


        <Grid Grid.Row="5"  VerticalAlignment="Bottom"  Background="#DD262726">
            <Grid x:Name="SearchBottomTab" Height="70" Visibility="Collapsed">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="8*"></ColumnDefinition>
                    <ColumnDefinition Width="2*"></ColumnDefinition>
                    <ColumnDefinition Width="8*"></ColumnDefinition>
                </Grid.ColumnDefinitions>
                <Grid Grid.Column="2" HorizontalAlignment="Left">
                    <Button x:Name="NextButton" Visibility="Collapsed" Background="Transparent" Command="{Binding SearchNextCommand}" CommandParameter="{Binding Text, ElementName=PageSearchTxtBox}"  Tag="NextPage"  Style="{StaticResource PdfViewerNavigationUpStyle }" >
                    </Button>
                </Grid>
                <Grid Grid.Column="0" HorizontalAlignment="Right">
                    <Button x:Name="PrevButton" Visibility="Collapsed" Background="Transparent" Command="{Binding SearchPreviousCommand}" CommandParameter="{Binding Text, ElementName=PageSearchTxtBox}" Tag="Prev" Style="{StaticResource PdfViewerNavigationDownStyle}" >
                    </Button>
                </Grid>
            </Grid>
        </Grid>

        <Grid Grid.Row="5" VerticalAlignment="Bottom" Height="50" Background="#DD262726" Name="GotoPageBottom" Visibility="Collapsed" >
            <Grid Width="Auto">
                <Border Background="#DD262726" BorderBrush="#FFCECECE" BorderThickness="1.5">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="8*" />
                            <ColumnDefinition Width="1*" />
                            <ColumnDefinition Width="1*" />
                        </Grid.ColumnDefinitions>

                        <Grid Grid.Column="0" Height="30" HorizontalAlignment="Stretch" >
                            <TextBox Margin="10,0,0,0" Name="PageDextTxtBox" FontFamily="Segoe UI" FontSize="13" Foreground="#FF0E0E0E" Text="Page Number" GotFocus="PageDextTxtBox_GotFocus" KeyDown="PageDextTxtBox_KeyDown"></TextBox>
                        </Grid>
                        <Grid Grid.Column="1" Height="30" HorizontalAlignment="Stretch">
                            <Button Command="{Binding GoToPageCommand}"  CommandParameter="{Binding Text, ElementName=PageDextTxtBox}">
                                <Path Width="12" Height="20" Data="M0.700004,0.7 L10.904,10.905 M0.7,20.0122 L10.904,9.80716" Stretch="Fill" Stroke="White" StrokeThickness="1.4" />
                            </Button>
                        </Grid>
                        <Grid Grid.Column="2" Height="30" Background="Transparent" Tapped="ClosePopUp" HorizontalAlignment="Left" >
                            <Button>
                                <Path Width="12" Height="20" Data="M0.750001,0.75 L27.457,27.457 M27.457,0.75 L0.75,27.457" Stretch="Fill" Stroke="White" StrokeThickness="1.5"  />
                            </Button>
                        </Grid>
                    </Grid>
                </Border>
            </Grid>
        </Grid>
    </Grid>
</Page>

